{
    "name": "liran-co/laravel-frontend-services",
    "description": "Easily add third-party services to your front-end.",
    "homepage": "https://github.com/liran-co/laravel-frontend-services",
    "keywords": [
        "laravel",
        "blade",
        "thirdparty"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Liran Cohen",
            "email": "l@liran.co"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "LiranCo\\FrontendServices\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiranCo\\FrontendServices\\FrontendServicesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
